{
    "title": "notes",
    "author": "Cao Rui",
    "description": "个人学习笔记",
    "language": "zh-hans",
    "root": "./docs",
    "styles":{
        "website": "styles/website.css"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "tbfed-pagefooter",
        "prism",
        "-highlight",
        "splitter",
        "expandable-chapters-small",
        "ace",
        "chapter-fold",
        "copy-code-button",
        "-sharing",
        "sharing-plus",
        "page-treeview",
        "back-to-top-button"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism.css"
            ]
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "pocket": false,
            "qq": false,
            "qzone": true,
            "twitter": false,
            "weibo": true,
            "all": [
                "douban",
                "facebook",
                "google",
                "instapaper",
                "linkedin",
                "twitter",
                "weibo",
                "messenger",
                "qq",
                "qzone",
                "viber",
                "whatsapp"
            ]
        },
        "page-treeview": {
            "copyright": "Copyright &#169; aleen42",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        }
    }
}